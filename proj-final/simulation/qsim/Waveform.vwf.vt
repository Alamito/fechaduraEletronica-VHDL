module Block1_vlg_vec_tst();
// constants                                           
// general purpose registers
reg I0;
reg I1;
reg I2;
reg I3;
// wires                                               
wire [15:0] output;

wire sampler;                             

// assign statements (if any)                          
Block1 i1 (
// port map - connection between master ports and signals/registers   
	.I0(I0),
	.I1(I1),
	.I2(I2),
	.I3(I3),
	.\output (output)
);

// I0
initial
begin
	I0 = 1'b1;
end 

// I1
initial
begin
	I1 = 1'b0;
end 

// I2
initial
begin
	I2 = 1'b1;
end 

// I3
initial
begin
	I3 = 1'b0;
end 

Block1_vlg_sample_tst tb_sample (
	.I0(I0),
	.I1(I1),
	.I2(I2),
	.I3(I3),
	.sampler_tx(sampler)
);

Block1_vlg_check_tst tb_out(
	.output(output),
	.sampler_rx(sampler)
);
endmodule

