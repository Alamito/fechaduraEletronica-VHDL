`timescale 1 ps/ 1 ps
module Block1_vlg_sample_tst(
	I0,
	I1,
	I2,
	I3,
	sampler_tx
);
input  I0;
input  I1;
input  I2;
input  I3;
output sampler_tx;

reg sample;
time current_time;
always @(I0 or I1 or I2 or I3)
                                                                                
begin                                                 
 if ($realtime > 0)                                   
 begin                                                
	if ($realtime == 0 || $realtime != current_time)  
	begin									          
		if (sample === 1'bx)                          
			sample = 0;                               
		else                                          
			sample = ~sample;                         
	end										          
	current_time = $realtime;					      
 end                                                  
end                                                   

assign sampler_tx = sample;
endmodule

